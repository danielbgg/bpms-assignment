package com.solarvillage.neworderpermitting.process_definition;

import com.solarvillage.neworderpermitting.data_model.NewOrder;

rule "EvaluateRequest"

	no-loop 
	ruleflow-group "evaluate"
	lock-on-active 
    
    when
        //conditions
        $order : NewOrder()
    then
        //actions
        modify($order) {setPrice(200)};

end